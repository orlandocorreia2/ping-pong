  "scripts": {
    "start": "node src/index.js",
    "docker:build": "docker image build -t account-properties:1.0 .",
    "docker:run": "docker container run -p 80:80 account-properties:1.0",
    "docker:start": "npm run docker:build && npm run docker:run",
